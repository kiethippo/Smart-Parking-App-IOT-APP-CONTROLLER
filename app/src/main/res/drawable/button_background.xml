<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/white"/> <!-- Màu khi nhấn -->
            <corners android:radius="12dp"/> <!-- Bo góc -->
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white"/> <!-- Màu mặc định -->
            <corners android:radius="12dp"/>
        </shape>
    </item>
</selector>